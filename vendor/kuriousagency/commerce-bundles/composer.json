{
	"name": "kuriousagency/commerce-bundles",
	"description": "Bundles plugin for Craft Commerce",
	"type": "craft-plugin",
	"version": "1.1.2",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"bundles"
	],
	"support": {
		"docs": "https://github.com/KuriousAgency/commerce-bundles/blob/master/README.md",
		"issues": "https://github.com/KuriousAgency/commerce-bundles/issues"
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "Kurious Agency",
			"homepage": "https://kurious.agency"
		}
	],
	"require": {
		"craftcms/cms": "^3.0.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"kuriousagency\\commerce\\bundles\\": "src/"
		}
	},
	"extra": {
		"name": "Bundles",
		"handle": "commerce-bundles",
		"hasCpSettings": false,
		"hasCpSection": true,
		"changelogUrl": "https://raw.githubusercontent.com/KuriousAgency/commerce-bundles/master/CHANGELOG.md",
		"components": {
			"bundlesService": "kuriousagency\\commerce\\bundles\\services\\BundlesService"
		},
		"class": "kuriousagency\\commerce\\bundles\\Bundles"
	}
}
